{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://www.linkedin.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0"
      }
    },
    {
      "urls": ["https://www.productplan.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s"
}
